{
  "Environment": {
    "Name": "Development",
    "BaseUrl": "https://playwright.dev",
    "ApiBaseUrl": "https://api.playwright.dev"
  },
  "Browser": {
    "Type": "chromium",
    "Headless": true,
    "SlowMo": 0,
    "Timeout": 30000,
    "Viewport": {
      "Width": 1920,
      "Height": 1080
    },
    "Screenshots": {
      "Enabled": true,
      "TakeOnFailure": true,
      "Directory": "TestResults/Screenshots"
    }
  },
  "Timeouts": {
    "PageLoad": 30000,
    "Navigation": 30000,
    "Element": 10000,
    "Script": 10000
  },
  "Trace": {
    "Enabled": true,
    "Directory": "TestResults/Traces",
    "Mode": "OnFailure",
    "Screenshots": true,
    "Snapshots": true,
    "Sources": true
  }
}
